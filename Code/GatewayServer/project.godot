config_version=4

[application]

config/name="Gateway"
run/main_scene="res://Scenes/Main/LoginServer.tscn"
config/icon="res://icon.png"

[autoload]

Gateway="*res://Scenes/Singeltons/Gateway.gd"
Authenticate="*res://Scenes/Singeltons/Authenticate.gd"

[display]

window/size/width=600
window/size/height=300

[editor_plugins]

enabled=PoolStringArray( "res://addons/Todo_Manager/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
